<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.connection.url">jdbc:derby://localhost:1527/wnioskizakupowe</property>
    <property name="hibernate.connection.driver_class">org.apache.derby.jdbc.ClientDriver</property>
    <property name="hibernate.connection.username">wnioskizakupowe</property>
    <property name="hibernate.connection.password">wnioskizakupowe</property>
    <property name="hibernate.dialect">org.hibernate.dialect.DB2Dialect</property>
    <mapping resource="hibernate/mapping/wniosek.hbm.xml"/>
    <mapping resource="hibernate/mapping/przedmiot_zamowienia.hbm.xml"/>
    <mapping resource="hibernate/mapping/przedmiot_zamowienia_item.hbm.xml"/>
    <mapping resource="hibernate/mapping/user.hbm.xml"/>
    <mapping resource="hibernate/mapping/uwagi.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
